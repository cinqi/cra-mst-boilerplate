import React, { Component } from 'react';
{{#if addModel}}
import { observer, inject } from 'mobx-react';
{{else}}
// Uncomment this line if you want to observe some store
//
// import { observer, inject } from 'mobx-react';
{{/if}}

{{#if addHeaders}}
import { Helmet } from 'react-helmet';
{{/if}}

/* eslint-disable react/prefer-stateless-function */
class {{ properCase name }} extends Component {
    render() {
        return (
            <>
                {{#if addHeaders}}
                <Helmet>
                    <title>{{ properCase name }}</title>
                    <meta name="description" content="Description of {{ properCase name }}"/>
                </Helmet>
                {{/if}}
                Content here
            </>
            );
    }
}

{{#if addModel}}
export default inject('{{ camelCase name }}DomainStore')(observer({{ properCase name }}));
{{else}}
// If you want to observe and inject another store to container, use export below:
// export default inject('storeName')(observer({{ properCase name }}));
export default {{ properCase name }};
{{/if}}